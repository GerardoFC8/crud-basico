models:
    Student: { nombres: 'string:100', correo: 'string:100 unique', cedula: 'string:20 unique', edad: 'integer nullable', telefono: 'string:20 nullable', direccion: 'string nullable', status: 'enum:activo,inactivo,graduado default:activo', softdeletes: softDeletes }
    Category: { name: 'string:100 unique', description: 'text nullable' }
    Post: { title: "string:255 default:'Untitled Post'", content: longtext, status: "enum:['draft', 'published', 'archived'] default:'draft'", is_featured: 'boolean default:false', views_count: 'integer unsigned default:0', published_at: 'timestamp nullable', softdeletes: softDeletes, relationships: { belongsTo: Category } }
